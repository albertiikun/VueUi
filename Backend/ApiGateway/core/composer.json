{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "laravel/lumen-framework": "^6.0",
        "nordsoftware/lumen-cors": "^3.2",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Functional\\": "../functional/",
            "Host\\": "../host/",
            "Domain\\": "../domain/",
            "Core\\": "../core/",
            "Persistence\\": "../persistence"
        },
        "classmap": [
            "../persistence/database/seeds",
            "../persistence/database/factories"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
