{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.4",
        "illuminate/mail": "^6.6",
        "laravel/lumen-framework": "^6.0",
        "ramsey/uuid": "^3.8",
        "spatie/async": "^1.1",
        "darkaonline/swagger-lume": "6.*",
        "league/flysystem-aws-s3-v3": "~1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Functional\\": "../functional/",
            "Host\\": "../host/",
            "Domain\\": "../domain/",
            "Core\\": "../core/",
            "Persistence\\": "../persistence/"
        },
        "classmap": [
            "../persistence/database/seeds",
            "../persistence/database/factories"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}