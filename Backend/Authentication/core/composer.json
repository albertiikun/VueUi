{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "dusterio/lumen-passport": "^0.2.15",
        "illuminate/mail": "6.6",
        "laravel/lumen-framework": "^6.0",
        "nordsoftware/lumen-cors": "^3.2",
        "piwik/device-detector": "^3.12",
        "torann/geoip": "^1.1",
        "ramsey/uuid": "^3.9",
        "darkaonline/swagger-lume": "6.*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Functional\\": "../functional/",
            "Host\\": "../host/",
            "Domain\\": "../domain/",
            "Core\\": "../core/",
            "Persistence\\": "../persistence"
        },
        "classmap": [
            "../persistence/database/seeds",
            "../persistence/database/factories"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
